{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Vertreter Created Event Signature",
    "type": "object",
    "properties": {
        "vertreterNr": {
            "type": "string",
            "minLength": 3
        },
        "name": {
            "type": "string",
            "minLength": 2
        },
        "status": {
            "type": "string",
            "enum": [
                "AKTIV",
                "INAKTIV"
            ]
        }
    },
    "required": [
        "vertreterNr",
        "name"
    ]
}